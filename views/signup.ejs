                                <i class="mdi mdi-alert-outline"></i>
                                <%= errorSignup %>
                            </div>
                            <% } %>
                            <div class="row">
                                <form action="/signup" method="POST" autocomplete="off" class="log-form">
                                    <div class="col-md-12 col-sm-12 col-xs-12">
                                        <input type="text" name="name" class="form-control" placeholder="Full Name" required data-error="Please enter your name">
                                    </div>
                                    <div class="col-md-12 col-sm-12 col-xs-12">
                                        <input type="text" name="username" class="form-control" placeholder="Your Email" required data-error="Please enter your name">
                                    </div>
                                    <div class="col-md-12 col-sm-12 col-xs-12">
                                        <input type="password" name="password" class="form-control" placeholder="Password" required data-error="Please enter your message subject"  minlength="8">
                                    </div>
                                    <div class="col-md-12 col-sm-12 col-xs-12 ">
                                        <div class="check-group flexbox">
                                            <label class="check-box">
                                                <input type="checkbox" class="check-box-input" checked>
                                                <span class="remember-text">I agree terms & conditions</span>
                                            </label>
                                        </div>
                                    </div>
                                    <div class="col-md-12 col-sm-12 col-xs-12">
                                        <button type="submit" class="login-btn">Sign up</button>
                                    </div>
                                    <div class="col-md-12 col-sm-12 col-xs-12">
                                        <div class="sign-icon">
                                            <div class="acc-not">have an account? <a href="login">Login</a></div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                        <!-- <div class="hidden-xs bnn hidden-sm">
