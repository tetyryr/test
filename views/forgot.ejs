    <div id="preloader"></div>
    <!-- header end -->
    <!-- Start breadcumb Area -->
    <!-- End breadcumb Area -->
    <!-- Start Slider Area -->
    <div class="w-100 d-flex justify-content-center align-items-center" style="background:url(/img/background/bg3.png); min-height:100vh">
        <div class=" w-100" id="bookn" tabindex="-1" role="dialog" aria-labelledby="dem" aria-hidden="true">
            <div class="modal-dialog  modal-lg  p-0" role="document">
                <div class="modal-content p-0" style="background:none; ">
                    <div class="d-flex flex-column align-items-center modal-body p-0 ">
                        <section class="row align-items-lg-center banner-stripe w-100 p-0" >
                            <div id="mc_embed_signup" class=" col-12 col-lg-6 banner-stripe-cover w-100 d-flex flex-column justify-content-center align-items-start px-5 py-lg-5 pl-lg-5 py-5">
                                <h4 style="color:#333" class="my-3 pr-3">
                                    Forgotten Password!!!</h4>
                                <% if(userexist.length > 0){ %>
                                <div class="d-flex flex-column align-items-start justify-content-center">
                                    <div class="alert alert-success " role="alert" style="font-size:12px">
                                        <i class="mdi mdi-warning"></i>
                                        <%= userexist %>
                                    </div>
                                    <h4 style="color:333" class="my-3 pr-3">
                                        A verification link has been sent to your email to verify its you.
                                    </h4>
                                </div>
                                <% } %>
                                <% if(userexist.length == 0){ %>
                                <h4 style="color:#333" class="my-5 pr-3">
                                    Input your email to change your password</h4>
                                <form action="/forgot" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate w-100" validate>
                                    <div id="mc_embed_signup_scroll" class="w-100">
                                        <input type="email" value="" name="username" class="email form-control form-control-lg in-sign mb-3 w-100" id="mce-EMAIL" placeholder="email address" required style="background:transparent; font-size: 20px">
                                        <% if(userexistnot.length > 0){ %>
                                        <div class="alert alert-danger " role="alert" style="font-size:12px">
                                            <i class="mdi mdi-warning"></i>
                                            <%= userexistnot %>
                                        </div>
                                        <% } %>
                                        <div class="clear"><input style="font-weight:700" type="submit" value="Submit"  class=" btn btn-warning btn-lg py-3 px-5" style="border:none;"></div>
                                    </div>
                                </form>
                                <% } %>
                                <br>
                                <div class="mt-3" style="text-decoration:underline;color:#ccc">
                                    <a href="/">Go back to Homepage <i class="mdi mdi-home-roof"></i></a>
                                </div>
                                <br>
                                
                            </div>
                            <div class="col-lg-6 col-12 m-0 p-0  booknm d-flex align-items-center justify-content-center" style="overflow: hidden;">
                                <img src="/img/background/b3.jpg" class="img-fluid" style="background-size: cover;">
                            </div>
                        </section>
                    </div>
                </div>
            </div>
        </div>
